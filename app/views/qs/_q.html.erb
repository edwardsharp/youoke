<% unless q.video_queue.blank? %>
  <% first_video = q.video_queue.first %>
  <% q.video_queue.each do |v| %>

    <%= "<b>Currently Playing:".html_safe if v == first_video %>

    <%= "<i>".html_safe if v.file_path.nil? %>
    <small><%= v.position unless v == first_video %></small> <%= v.id %> 
    <%= "</i>".html_safe if v.file_path.nil? %>
    
    <%= "</b>".html_safe if v == first_video %>
  
         
    <%= link_to 'skip', channel_skip_path(q.channel, v.id), remote: true %>

    <br>
  <% end -%>

<% end -%>