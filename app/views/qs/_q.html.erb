<% unless q.videos.blank? %>

  <% q.videos.each do |v| %>

    <%= "<b>Currently Playing:".html_safe if v == q.videos.first %>

    <%= "<i>".html_safe if v.file_path.nil? %>
    <small><%= v.position unless v == q.videos.first %></small> <%= v.id %> 
    <%= "</i>".html_safe if v.file_path.nil? %>
    
    <%= "</b>".html_safe if v == q.videos.first %>
      

    <br>
  <% end -%>

<% end -%>