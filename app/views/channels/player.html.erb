<section id="player" data-channel="player" data-channel-id="<%= @channel.id %>" data-sync-user-id="<%= @channel.sync_user_id %>">
  <%= render 'player', channel: @channel %>
</section>

<div id="playerQ">
<%= render 'qs/qs', channel: @channel %>
</div>

<div id="playerChannel">
<div id="channelContent">
  <%= @channel.title %>
  <br>
  <%= @channel.content %>
  <div id="isSyncUser" class="<%= 'hidden' unless @channel.sync_user_id == @current_user.id %>">
    <i class="material-icons">star_border</i>
  </div>
</div>  
</div>

<%= render 'comments/player_comments', channel: @channel %>
