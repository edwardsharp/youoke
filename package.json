        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "youoke"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "edwardsharp",
        "name": "youoke"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "youoke"
      }
    }
