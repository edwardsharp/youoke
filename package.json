  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint src"
  },
  "keywords": ["youtube","karaoke"],
  "author": "3dwardsharp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/edwardsharp/youoke/issues"
  },
  "homepage": "https://github.com/edwardsharp/youoke#readme",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
